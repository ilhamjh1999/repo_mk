<odoo>
  <data>
  	
  	 <record id="view_project_inherit_form" model="ir.ui.view">
        <field name="name">project.form.view.inherit</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project"></field>
        <field name="arch" type="xml">
             <field name="partner_id" position="after">
                    <field name="location"/>
                    <field name="description"/>
			</field>
			<field name="user_id" position="after">
                    <field name="nilai"/>
			</field>
        </field>
    </record>
	<record id="view_project_inherit_tree" model="ir.ui.view">
        <field name="name">project.tree.view.inherit</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.view_project"></field>
        <field name="arch" type="xml">
             <field name="partner_id" position="after">
                    <field name="location"/>
			</field>
        </field>
    </record>		

  </data>
</odoo>  	
  	